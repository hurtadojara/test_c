#!/bin/bash

echo "-----------------------------------------------------------------------------------"
echo "-----------------------------------------------------------------------------------" 
echo "Simple_shell"
echo "pwd" | ./hsh
sleep 2
echo "-----------------------------------------------------------------------------------"
echo "Shell"
echo "pwd" | sh
sleep 2
echo "-----------------------------------------------------------------------------------" 
echo "-----------------------------------------------------------------------------------" 
echo -e "\n\n"

echo "-----------------------------------------------------------------------------------"
echo "-----------------------------------------------------------------------------------"
echo "Simple_shell"
echo "exit" | ./hsh
sleep 2
echo "-----------------------------------------------------------------------------------"
echo "Shell"
echo "exit" | sh
sleep 2
echo "-----------------------------------------------------------------------------------"
echo "-----------------------------------------------------------------------------------"
echo -e "\n\n"

echo "-----------------------------------------------------------------------------------"
echo "-----------------------------------------------------------------------------------"
echo "Simple_shell"
echo "    " | ./hsh
sleep 2
echo "-----------------------------------------------------------------------------------"
echo "Shell"
echo "    " | ./ sh
sleep 2
echo "-----------------------------------------------------------------------------------"
echo "-----------------------------------------------------------------------------------"
echo -e "\n\n"
